{
    "configurations": [
        {
            "type": "node-terminal",
            "name": "Test",
            "request": "launch",
            "command": "npm run test",
            "cwd": "${workspaceFolder}",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node-terminal",
            "name": "Debug",
            "request": "launch",
            "command": "npm run debug",
            "cwd": "${workspaceFolder}",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node-terminal",
            "name": "Debug new Build",
            "request": "launch",
            "command": "npm run debug:newbuild",
            "cwd": "${workspaceFolder}",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node-terminal",
            "name": "Build",
            "request": "launch",
            "command": "npm run build",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "node-terminal",
            "name": "Start",
            "request": "launch",
            "command": "npm run start",
            "cwd": "${workspaceFolder}",
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}