	"bugs": {
		"url": "https://github.com/LeiCraft/LeiCoin-Node/issues"
	},
	"homepage": "https://github.com/LeiCraft/LeiCoin-Node#readme",
	"dependencies": {
		"@elysiajs/cors": "^1.1.0",
		"@noble/secp256k1": "^2.1.0",
		"ansi-escapes": "^4.3.2",
		"bun": "^1.1.26",
		"chalk": "^4.1.2",
		"classic-level": "^1.4.1",
		"dotenv": "^16.3.1",
		"elliptic": "^6.5.5",
		"elysia": "^1.1.9",
		"node-cron": "^3.0.3",
		"patch-package": "^8.0.0"
	},
	"devDependencies": {
		"@types/bun": "^1.1.6",
		"@types/cors": "^2.8.17",
		"@types/elliptic": "^6.4.18",
		"@types/node": "^20.10.1",
		"@types/node-cron": "^3.0.11",
		"typescript": "^5.3.3"
	}
